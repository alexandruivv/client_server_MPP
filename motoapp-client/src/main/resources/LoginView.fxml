<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="motoapp.client.gui.LoginController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <BorderPane prefHeight="575.0" prefWidth="797.0" style="-fx-background-color: #FBC381;">
         <top>
            <VBox prefHeight="301.0" prefWidth="797.0">
               <children>
                  <HBox alignment="TOP_RIGHT" prefHeight="28.0" prefWidth="797.0" style="-fx-background-color: #FBC381;">
                     <children>
                        <Label alignment="CENTER" opacity="0.24" prefHeight="20.0" prefWidth="90.0" style="-fx-font-family: &quot;Arial&quot;;" text="MOTORACE">
                           <HBox.margin>
                              <Insets right="670.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                        <ImageView fitHeight="30.0" fitWidth="35.0" onMouseClicked="#on_btnClose_clicked" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@close.png" />
                           </image>
                           <effect>
                              <DropShadow />
                           </effect>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </ImageView>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="262.0" prefWidth="797.0" style="-fx-background-color: #FBC589;">
                     <children>
                        <ImageView fitHeight="226.0" fitWidth="355.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
                           <image>
                              <Image url="@motor.png" />
                           </image>
                           <BorderPane.margin>
                              <Insets top="20.0" />
                           </BorderPane.margin>
                        </ImageView>
                     </children>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                     <effect>
                        <DropShadow color="#ae977d" height="8.95" radius="4.07" width="9.33" />
                     </effect>
                  </HBox>
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </VBox>
         </top>
         <center>
            <VBox prefHeight="340.0" prefWidth="797.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER" prefHeight="371.0" prefWidth="797.0">
                     <children>
                        <VBox prefHeight="133.0" prefWidth="55.0">
                           <children>
                              <ImageView fitHeight="34.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@icon-user.png" />
                                 </image>
                                 <VBox.margin>
                                    <Insets left="5.0" top="5.0" />
                                 </VBox.margin>
                              </ImageView>
                              <ImageView fitHeight="41.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@lock.png" />
                                 </image>
                                 <VBox.margin>
                                    <Insets top="30.0" />
                                 </VBox.margin>
                              </ImageView>
                           </children>
                        </VBox>
                        <VBox prefHeight="242.0" prefWidth="229.0">
                           <children>
                              <TextField fx:id="usernameText" prefHeight="37.0" prefWidth="229.0" style="-fx-background-color: #FBC382; -fx-border-color: black; -fx-font-family: &quot;Raleway&quot;;" text="Username" />
                              <PasswordField fx:id="passwordText" style="-fx-background-color: #FBC381; -fx-border-color: black; -fx-font-family: &quot;Raleway&quot;;" text="Password">
                                 <VBox.margin>
                                    <Insets top="35.0" />
                                 </VBox.margin>
                              </PasswordField>
                           </children>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets top="60.0" />
                     </VBox.margin>
                  </HBox>
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </VBox>
         </center>
         <bottom>
            <Button mnemonicParsing="false" onAction="#on_btnLogIn" prefHeight="31.0" prefWidth="131.0" style="-fx-background-color: #f26834; -fx-font-family: &quot;Raleway&quot;;" text="Login" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="50.0" />
               </BorderPane.margin>
               <effect>
                  <DropShadow />
               </effect>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </bottom>
      </BorderPane>
   </children>
</GridPane>
